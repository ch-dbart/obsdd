#env
LESS = -firs -x 4 -P %t

#command
/ forw-search
:e visual
:h help
:n next-file
:p prev-file
? back-search
G goto-end
N reverse-search
^B back-screen
^D forw-scroll
^F forw-screen
^L repaint
^U back-scroll
g goto-line
h left-scroll
j forw-line
k back-line
l right-scroll
n repeat-search
q quit
{ forw-bracket {}
} back-bracket {}
( forw-bracket ()
) forw-bracket ()
] forw-bracket []
[ forw-bracket []
#stop

#line-edit
#stop
